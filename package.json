{
  "name": "@klasa/ws",
  "version": "0.0.9",
  "description": "WebSocket manager that handles spawning and maintaining a connection to the Discord gateway",
  "main": "dist/index.js",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "tsc",
    "test": "ava",
    "test:lint": "eslint --ext ts src test",
    "test:coverage": "npx nyc check-coverage --lines 0 --functions 0 --branches 0",
    "coverage": "npx nyc --require ts-node/register --require source-map-support/register npm test",
    "coverage:report": "npx nyc report --reporter=html",
    "lint": "eslint --fix --ext ts src test",
    "docs": "typedoc"
  },
  "keywords": [],
  "author": "dirigeants",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "dist/*"
  ],
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@klasa/dapi-types": "0.0.10",
    "@klasa/rest": "0.3.2",
    "@types/node": "^14.0.1",
    "@types/ws": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "ava": "^3.8.2",
    "eslint": "^6.8.0",
    "eslint-config-klasa": "github:dirigeants/klasa-lint",
    "nyc": "^15.0.1",
    "source-map-support": "^0.5.16",
    "ts-node": "8.10.1",
    "typedoc": "^0.17.6",
    "typescript": "^3.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dirigeants/ws"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "test/**/*.ts",
      "!test/lib"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "dependencies": {
    "@klasa/async-queue": "^0.0.1",
    "@klasa/bitfield": "^0.0.2",
    "@klasa/cache": "^0.0.2",
    "@klasa/utils": "^0.0.4",
    "ws": "^7.3.0"
  },
  "peerDependencies": {
    "@klasa/rest": "^0.3.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.1",
    "utf-8-validate": "^5.0.2",
    "zlib-sync": "^0.1.6"
  }
}
